@model InvoiceFilterViewModel

@{
    ViewBag.Title = "Invoice & DDT Summary";
    if (!string.IsNullOrWhiteSpace(CurrentUser.CxmlFormPost))
    {
        Layout = "~/Views/Shared/_LayoutExternal.cshtml";
    }
}
@if (string.IsNullOrWhiteSpace(CurrentUser.CxmlFormPost))
{
    <script src="https://maps.googleapis.com/maps/api/js?key=@(SiteFuel.Exchange.Core.Utilities.AppSettings.GoogleMapApiKey)" type="text/javascript"></script>
}
@{ Html.RenderPartial("_PartialJobInvoiceDetails", Model);}