                if ($('input[name="@Html.NameFor(m=> m.FuelDetails.FuelQuantity.QuantityTypeId)"]:checked').val() != '@((int)QuantityType.NotSpecified)') {
                    isvalid = isvalid && validateDeliverySchedule() && isWeekendAndSpecialPriceValid
