@using GridMvc.Html;

@model IEnumerable<SupplierWorkingHoursViewModel>

@using (Html.BeginCollectionItem("SupplierWorkingHours"))
{

    @Html.Grid(Model).Columns(columns =>
    {
        columns.Add(m => m.AddressId, true);
        columns.Add(m => m.WeekDayId, true);
        columns.Add(m => m.WeekDayName).Titled(Resource.gridColumnWeekDay);
        columns.Add(m => m.StartTime).Titled(Resource.gridColumnStartTime);
        columns.Add(m => m.EndTime).Titled(Resource.gridColumnEndTime);
    })

}
