@model SupplierWorkingHoursViewModel

@using (Html.BeginCollectionItem("SupplierWorkingHours"))
{
    @Html.HiddenFor(m => m.AddressId)
    @Html.HiddenFor(m => m.WeekDayId)
    @Html.HiddenFor(m => m.WeekDayName)
    <tr>
        <td width="30%">
            @Html.DisplayFor(m => m.WeekDayName)
        </td>
        <td>
            @Html.TextBoxFor(m => m.StartTime, new { @class = "form-control timepicker", placeholder = @Html.DisplayNameFor(m => m.StartTime) })
            @Html.ValidationMessageFor(m => m.StartTime)
        </td>
        <td>
            @Html.TextBoxFor(m => m.EndTime, new { @class = "form-control timepicker", placeholder = @Html.DisplayNameFor(m => m.EndTime) })
            @Html.ValidationMessageFor(m => m.EndTime)
        </td>
    </tr>
}
