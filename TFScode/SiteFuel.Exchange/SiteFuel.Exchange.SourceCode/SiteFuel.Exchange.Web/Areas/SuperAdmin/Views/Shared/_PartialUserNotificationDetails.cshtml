@model NotificationSettingsViewModel

@foreach (var notificationItem in Model.EventGroupDetails)
{
    <tr>
        <th colspan="6" class="bg-lightgrey">@notificationItem.EventGroupName</th>
    </tr>
    foreach (var item in notificationItem.EventDetails)
    {
        @Html.Partial("_PartialNotificationDetails", item)
    }
}