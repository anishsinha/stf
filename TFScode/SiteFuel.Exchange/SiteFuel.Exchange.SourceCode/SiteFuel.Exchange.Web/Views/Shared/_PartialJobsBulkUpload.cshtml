<div class="btn-group mt2">
    <button data-toggle="dropdown" class="btn btn-default btn-sm dropdown-toggle uploadInBulkBtn" aria-expanded="false" onclick="toggleElement('createNew');"><i class="fa fa-cloud-upload-alt bulkElements mr5"></i> @Resource.btnLabelJobsBulkUpload</button>
</div>
<div class="col-sm-8 text-left createNew upload-assets hide-element right0 mr15 mt1 pa radius-5 bg-white border z-index5 shadow-b">
    <i class="fa fa-times float-right mt5" aria-hidden="true" onclick="toggleElement('createNew');"></i>
    <span class="fa fa-download mr10 mt10"></span><a href="~/Content/Job_Bulkupload_Template.csv" class="mb5 btn-download">@Resource.btnLabelDownloadSample</a>
    <div class="row">
        <div class="col-md-12 b-dashed">
            <div class="well well-xs pt0 pb0 mt5">
                <h2>
                    <label for="csvFile" class="btn btn-primary">
                        <input id="csvJobsFile" name="csvFile" class="bulkElements full-width" type="file" accept=".csv" />
                    </label>
                </h2>
                <div class="row hide-element" id="divAssetFileSizeError">
                    <div class="col-sm-12 fs12 color-red">
                        @Resource.errFileSizeMessage
                    </div>
                </div>
                <ul class="list-unstyled m-t-md fs12">
                    <li>
                        @Resource.lblbulkJobUploadWarning
                    </li>
                </ul>
                <div class="row">
                    <div class="col-sm-12 text-right pb15">
                        <input type="button" value="@Resource.btnLabelUpload" class="btn btn-primary bulkElements" id="uploadJobsButton" />
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>