@model SupplierSourceViewModel

@Html.HiddenFor(m => m.Id)
@Html.TextBoxFor(m => m.Name, new { @class = "form-control supplier-source-name", @placeholder = @Resource.lblSupplierSource })
<script>
    $(document).ready(function () {
        var supplierSourceUrl = "@Url.Action("GetSupplierSource", "Dispatch", new { area = "Supplier" })";
        autoCompleteTextBox("#@Html.IdFor(m => m.Name)", supplierSourceUrl);
    });
</script>
