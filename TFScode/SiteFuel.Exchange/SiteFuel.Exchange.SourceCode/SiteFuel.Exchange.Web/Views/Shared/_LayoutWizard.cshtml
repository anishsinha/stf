<!DOCTYPE html>
<html>
<head>
    @RenderSection("header", required: false)
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <!-- The above 3 meta tags *must* come first in the head; any other head content must come *after* these tags -->
    <meta name="description" content="TrueFill Exchange Application">
    <meta name="author" content="TrueFill, Inc.">
    <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,400,600,700,800" rel="stylesheet">
    @if (!string.IsNullOrEmpty(ViewBag.FaviconPath))
    {
        <link rel="icon" href="@ViewBag.FaviconPath">
    }
    else
    {
        <link rel="icon" href="~/Content/images/logo-favicon.png">
    }

    <title>TrueFill - @ViewBag.Title</title>

    <!-- CSS -->

    @Styles.RenderFormat(SiteKeys.StyleVersion, "~/Content/css/datatables")
    @Styles.RenderFormat(SiteKeys.StyleVersion, "~/Content/css/bootstrap")
    @Styles.RenderFormat(SiteKeys.StyleVersion, "~/Content/css/site")
    @Styles.RenderFormat(SiteKeys.StyleVersion, "~/Content/css/font-awesome")
    @Styles.RenderFormat(SiteKeys.StyleVersion, "~/Content/css/toast")
    @RenderSection("styles", required: false)
    @*@Styles.RenderFormat(SiteKeys.StyleVersion, "~/Content/css/master-empty")*@


    <!-- JS -->
    @Scripts.RenderFormat(SiteKeys.ScriptVersion, "~/Content/js/jquery")
    @Scripts.RenderFormat(SiteKeys.ScriptVersion, "~/Content/js/jquery-mask")
    @Scripts.RenderFormat(SiteKeys.ScriptVersion, "~/Content/js/bootstrap")
    @Scripts.RenderFormat(SiteKeys.ScriptVersion, "~/Content/js/toast")
    @Scripts.RenderFormat(SiteKeys.ScriptVersion, "~/Content/js/site")
    @*@Scripts.RenderFormat(SiteKeys.ScriptVersion, "~/Content/js/master-dashboard")
        @Scripts.RenderFormat(SiteKeys.ScriptVersion, "~/Content/js/dataTables")*@
    @RenderSection("scripts", required: false)
</head>
<body>
    @*<header class="fixed full-width top0 bg-white" id="layoutHeader">
        <div class="container-fluid">
            <div class="row">
                <div class="col-8 pr0 col-md-6 col-lg-7">
                    <div class="float-left mt3 ml10 d-none d-lg-block"><a href="/"><img src="~/Content/images/truefill-logo.png" width="125" role="button" alt="TrueFill" class="mt5"></a></div>
                    <div class="float-left mt3 ml5  d-none d-lg-none d-sm-block d-md-block d-block d-sm-none"><img src="~/Content/images/truefill-symbol.png" width="25" role="button" alt="TrueFill" class="ml5"></div>
                </div>
            </div>
        </div>
    </header>*@
        <div class="container-fluid page-bg">
            @RenderBody()
        </div>
    @Html.PageScripts()
    @*<script>
        var breadCrumbUrl = '@Url.Action("GetBreadcrumbMapping", "Home", new { area = ""})';
        </script>*@
</body>
</html>

