@model CounterOfferViewModel

@Html.Partial("CounterOfferDetails/_PartialQuantity", Model)

@Html.Partial("CounterOfferDetails/_PartialPrice", Model)

@Html.Partial("CounterOfferDetails/_PartialFeeSection", Model)

@Html.Partial("CounterOfferDetails/_PartialDeliveryDetails", Model)

@Html.Partial("CounterOfferDetails/_PartialPayment", Model)

@Html.Partial("CounterOfferDetails/_PartialSpecialInstruction", Model)

@Html.Partial("CounterOfferDetails/_PartialComment", Model)

<script>
    $(document).ready(function () {
        var columns = $(".offer-columns > .offer-container");
        $(columns).each(function () {
            var columnNumber = $(this).find("> div").length;
            var latestOffer = $(this).find("> div.single-record:nth-child(" + columnNumber + ") p span, > div.single-record:nth-child(" + columnNumber + ") ol li span").text().trim().replace(/\s+/g, '');
            var prevOffer = $(this).find("> div.single-record:nth-child(" + (columnNumber - 1) + ") p span, > div.single-record:nth-child(" + (columnNumber - 1) + ") ol li span").text().trim().replace(/\s+/g, '');
            if (latestOffer != prevOffer) {
                $(this).find("> div.single-record:nth-child(" + columnNumber + ") p span, > div.single-record:nth-child(" + columnNumber + ") ol li span").addClass("label-danger color-white");
            }
        });
    });
</script>