@model PricingViewModel
@using (Html.BeginCollectionItem("Details.FuelPricing.TierPricing.Pricings"))
{
    <div class="partial-block">
        <div class="row">
            <div class="col-sm-12">
                @Html.PartialFor("_PartialTierFuelQuantityBroker", m => m)
            </div>
        </div>
    </div>
}
<script type="text/javascript">
    $(document).ready(function () {
        var rowIdx = parseInt($('#TotalRows').val());
        rowIdx = ++rowIdx;
        $('#TotalRows').val(rowIdx);
    });
</script>