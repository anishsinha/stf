@using SiteFuel.Exchange.Core.StringResources;
@using SiteFuel.Exchange.Utilities;
@model SiteFuel.Exchange.ViewModels.ProgressReportViewModel
@using System.Threading
@{
    Thread.CurrentThread.CurrentUICulture = new System.Globalization.CultureInfo(Model.Culture);
}
<table cellspacing="0" cellpadding="0">
    <tr>
        <td align="center"><img src="@Model.DailyReportLogo" width="30%"></td>
    </tr>
    <tr>
        <td>
            <p style="margin-top:30px;margin-bottom:25px">
                <h1 style="text-align:center;margin-top:0px;margin-bottom:0px;font-weight:600;font-size:26px;">@Resource.lblSiteFuelDailySummary</h1>
                <h4 style="text-align:center;margin-top:10px;margin-bottom:0px;font-weight:400">@Model.Date</h4>
            </p>
        </td>
    </tr>
    @if (Model.ProgressReportCount.AccountOwnerName != "")
    {
        <tr>
            <td>
                <h2 style="margin-top:10px;margin-bottom:0px;text-align:center;font-weight:600;color:#2e97d1;font-size:16px;">@Model.ProgressReportCount.AccountOwnerName</h2>
            </td>
        </tr>
    }
    <tr>
        <td>
            <h2 style="margin-top:25px;margin-bottom:25px;text-align:center;font-weight:600;color:#4b4b4b;font-size:16px;">@Resource.lblAccountAndUserInformation</h2>
        </td>
    </tr>
    <tr>
        <td>
            <table width="100%" border="0" cellpadding="0" cellspacing="0">
                <tr>
                    <td width="47%" style="padding-right:2%" valign="top">
                        <table width="100%" cellpadding="0" cellspacing="0">
                            @if (!Model.HideActiveAccountsAndUsers)
                            {
                                <tr>
                                    <td style="font-weight:600;color:#4b4b4b;font-size:14px;">@Resource.lblActiveAccounts:</td>
                                    <td style="text-align:right;font-weight:600;color:#2e97d1;font-size:14px;">@Model.ProgressReportCount.ActiveAccounts</td>
                                </tr>
                            }
                            <tr>
                                <td style="font-weight:600;color:#4b4b4b;font-size:14px;">@Resource.lblNewAccounts:</td>
                                <td style="text-align:right;font-weight:600;color:#2e97d1;font-size:14px;">@Model.ProgressReportCount.NewAccounts</td>
                            </tr>
                            <tr>
                                <td style="font-weight:600;color:#4b4b4b;font-size:14px;">@Resource.lblNewBuyers:</td>
                                <td style="text-align:right;font-weight:600;color:#2e97d1;font-size:14px;">@Model.NewBuyers.Where(t => t.AccountTypeId == (int)AccountType.Real).Count()</td>
                            </tr>
                            <tr>
                                <td style="font-weight:600;color:#4b4b4b;font-size:14px;">@Resource.lblNewSuppliers:</td>
                                <td style="text-align:right;font-weight:600;color:#2e97d1;font-size:14px;">@Model.NewSuppliers.Where(t => t.AccountTypeId == (int)AccountType.Real).Count()</td>
