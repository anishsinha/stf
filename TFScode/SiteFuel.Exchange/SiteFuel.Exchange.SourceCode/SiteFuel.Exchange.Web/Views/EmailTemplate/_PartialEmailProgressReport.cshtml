                }
                else
                {
                    <div>@buyer.CompanyName @Resource.lblSingleHyphen @buyer.AdminUser @Resource.lblSingleHyphen @buyer.City @Resource.lblSingleHyphen @buyer.State</div>
                }
            }
        </td>
    </tr>
