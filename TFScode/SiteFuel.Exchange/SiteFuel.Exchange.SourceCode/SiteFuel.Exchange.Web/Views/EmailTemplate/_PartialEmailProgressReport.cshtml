                            <tr>
                                <td style="font-weight:600;color:#4b4b4b;font-size:14px;">@Resource.lblNewBuyerAndSuppliers:</td>
                                <td style="text-align:right;font-weight:600;color:#2e97d1;font-size:14px;">@Model.NewBuyerAndSuppliers.Where(t => t.AccountTypeId == (int)AccountType.Real).Count()</td>
                            </tr>
                            <tr>
                                <td style="font-weight:600;color:#4b4b4b;font-size:14px;">@Resource.lblSFXOwned:</td>
                                <td style="text-align:right;font-weight:600;color:#2e97d1;font-size:14px;">@Model.ProgressReportCount.SFXOwnedAccounts</td>
                            </tr>
